{
    "type": "form",
    "title": "Form Basic",
    "primaryKey": "id",
    "domain": "basic",
    "properties": [
      {
        "component": "h3",
        "children": "Fake Rest API"
      },
      {
        "name": "id",
        "label": "ID",
        "readonly": true,
        "config": {
          "grid": true,
          "sort": 1
        }
      },
      {
        "name": "id",
        "label": "ID",
        "readonly": true,
        "config": {
          "grid": true,
          "sort": 1,
          "type": "action",
          "action": {
            "handler": "https://google.com",
            "target": "_blank",
            "label": "<span title='{data}'>#ID</span>"
          }
        }
      },
      {
        "component": "div",
        "class": "d-flex",
        "children": [
          {
            "name": "autocomplete",
            "label": "Autocomplete",
            "type": "autocomplete",
            "config": {
              "grid": true,
              "sort": 1,
              "type": "select"
            },
            "options": [
              {
                "value": 1,
                "label": "Jon Doe"
              },
              {
                "value": 2,
                "label": "Jane Roe"
              }
            ]
          },
          {
            "name": "dynamicselect",
            "label": "Dynamic Select",
            "type": "select",
            "options": [],
            "attributes": {
              "url": "/examples/advanced_data.json",
              "fieldLabel": "name",
              "fieldValue": "id",
              "wrapData": "users",
              "requestOptions": {
                "headers": {
                  "app-key": "qwertyuiop"
                }
              }
            },
            "config": {
              "grid": true,
              "type": "select"
            }
          },
          {
            "name": "switch",
            "label": "Switch",
            "type": "switch",
            "config": {
              "grid": true,
              "type": "switch"
            }
          }
        ]
      }
    ],
    "api": {
      "rootApi": "/examples/advanced_data.json",  
      "headers": {
        "Accept": "application/json"
      }
    }
  }